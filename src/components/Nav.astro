---
// libraries import
import { Icon } from "astro-icon";
import Link from "./Link.astro";

//data imports
import navData from "../data/navData.js";
---

<nav aria-label="Primary">
  <div class="navbar">
    <a href="/" class="logomark" aria-label="Go Home">
      <Icon name="logomark" width="60" />
    </a>
    <ul>
      {
        navData.map((item) => (
          <Link
            text={item.name}
            href={item.path}
            style="primary"
            isFilled={false}
            data-navLink
          />
        ))
      }
    </ul>
  </div>
</nav>

<script>
  import "../js/nav.js"
</script>
